{
  "apiVersion": 2,
  "name": "eazydocs/shortcode",
  "version": "0.1.0",
  "title": "EazyDocs Shortcode",
  "category": "eazydocs",
  "icon": "media-document",
  "description": "Display the Docs on the website frontend.",
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "col": {
      "type": "number",
      "default": 3
    },
    "include": {
      "type": "array",
      "default": []
    },
    "exclude": {
      "type": "string"
    },
    "show_docs": {
      "type": "number"
    },
    "show_articles": {
      "type": "number"
    },
    "more": {
      "type": "string",
      "default": "View Details"
    }
  },
  "textdomain": "eazydocs",
  "editorScript": "file:./index.js"
}